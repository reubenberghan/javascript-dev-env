{
  "name": "javascript-dev-env",
  "version": "0.1.0",
  "description": "JavaScript development starter kit",
  "scripts": {
    "prestart": "babel-node build-scripts/startMessage.js",
    "start": "run-p security-check open:src lint:watch test:watch",
    "security-check": "nsp check",
    "open:src": "babel-node build-scripts/srcServer.js",
    "lint": "esw webpack.config.* src build-scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reubenberghan/javascript-dev-env.git"
  },
  "keywords": [
    "javascript",
    "development",
    "envirnoment",
    "starter",
    "kit"
  ],
  "author": "Reuben Berghan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reubenberghan/javascript-dev-env/issues"
  },
  "homepage": "https://github.com/reubenberghan/javascript-dev-env#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.0.1",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "chalk": "2.4.1",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-watch": "3.1.5",
    "express": "4.16.3",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.0.1",
    "npm-run-all": "4.1.3",
    "nsp": "3.2.1",
    "open": "0.0.5",
    "regenerator-runtime": "0.11.1",
    "style-loader": "0.21.0",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-middleware": "3.1.3"
  }
}
